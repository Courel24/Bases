        "@types/redis": "2.8.28",
        "@types/uniqid": "5.3.0",
        "apollo-server-express": "2.21.0",
        "aws-cognito-jwt-verifier": "^1.3.7",
        "bcrypt": "5.0.1",
        "class-validator": "0.13.1",
        "cookie-parser": "1.4.5",
        "cors": "2.8.5",
        "dotenv": "^8.2.0",
        "express": "4.17.1",
        "graphql": "15.5.0",
        "jsonwebtoken": "8.5.1",
        "module-alias": "2.2.2",
        "node-cron": "2.0.3",
        "pg": "8.5.1",
