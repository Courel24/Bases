        "eslint": "7.21.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-plugin-import": "2.22.1",
        "faker": "5.4.0",
        "husky": "5.1.3",
        "jest": "26.6.3",
        "nodemon": "2.0.7",
        "prettier": "2.2.1",
        "reflect-metadata": "0.1.13",
